{
  "name": "rasterex-space-management-api",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "populate": "ts-node -r tsconfig-paths/register src/populate.ts",
    "serve": "nodemon -w ./src -e ts node_modules/.bin/ts-node src/main.ts"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.2",
    "@nestjs/core": "^6.0.2",
    "@nestjs/platform-express": "^6.0.2",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/testing": "^6.0.2",
    "@nestjs/typeorm": "^6.0.0",
    "@nestjsx/crud": "^3.2.0",
    "@types/dotenv": "^6.1.1",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "dotenv": "^7.0.0",
    "js-yaml": "^3.13.1",
    "pg": "^7.9.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "rxjs": "^6.3.3",
    "swagger-ui-express": "^4.0.2",
    "typeorm": "^0.2.16"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/faker": "^4.1.5",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.14.4",
    "@types/supertest": "^2.0.7",
    "axios": "^0.18.0",
    "faker": "^4.1.0",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0",
    "supertest": "^3.4.1",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.12.1",
    "typescript": "^3.4.3"
  }
}
